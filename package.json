{
  "name": "react-aria-offcanvas",
  "version": "1.2.1",
  "description": "Accessible Off-Canvas component for React.js",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build": "webpack --mode production",
    "deploy": "gh-pages -d examples/__build__",
    "publish:gh": "npm run build && npm run deploy",
    "transpile": "npx babel src -d dist",
    "prepublish": "npm run transpile",
    "lint": "eslint src tests",
    "jest": "npx jest",
    "jest:w": "npx jest --watch",
    "test": "npm run lint && npm run jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neosiae/react-aria-offcanvas.git"
  },
  "keywords": [
    "react",
    "react-component",
    "aria",
    "offcanvas"
  ],
  "author": "Denis Kotlica",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neosiae/react-aria-offcanvas/issues"
  },
  "homepage": "https://neosiae.github.io/react-aria-offcanvas",
  "dependencies": {
    "prop-types": "^15.6.1",
    "run-event-handler-once": "^1.0.0"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "react-testing-library": "^7.0.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  }
